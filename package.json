    "@fontsource/roboto-slab": "^4.1.0",
    "@sanity/block-content-to-html": "^1.3.8",
    "@sanity/client": "^2.1.0",
    "@sanity/image-url": "^0.140.19",
    "@types/node": "^14.14.21",
    "firebase": "^8.2.3",
    "vue": "^3.0.5",
    "vue-router": "4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vitejs/plugin-vue": "^1.0.4",
