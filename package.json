    "@vue/eslint-config-typescript": "^7.0.0",
    "@vuedx/typecheck": "^0.4.1",
    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "babel-eslint": "^10.0.1",
    "browserslist": "^4",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "husky": "^5.0.6",
    "postcss": "^8.2.1",
